{
  "name": "@clockworksproduction-studio/cwp-open-terminal-emulator",
  "version": "5.1.6",
  "description": "A modular, BASH-style terminal emulator library for the web.",
  "type": "module",
  "main": "src/index.js",
  "module": "src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./core/*": "./src/core/*.js",
    "./boot/*": "./src/boot/*.js",
    "./vfs/*": "./src/vfs/*.js",
    "./addons/*": "./src/addons/*.js"
  },
  "bin": {
    "cwp-terminal-setup": "bin/cwp-terminal-setup.cjs",
    "cwp-vfs-importer": "bin/vfs-importer.cjs"
  },
  "files": [
    "src",
    "bin",
    "docs",
    "LICENSE",
    "README.md",
    "CONTRIBUTING.md",
    "CODE_OF_CONDUCT.md"
  ],
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "postinstall": "node bin/cwp-terminal-setup.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ClockWorksProduction/CWPs-OpenSource-BASHstyle-Terminal-emulator.git"
  },
  "keywords": [
    "terminal",
    "emulator",
    "shell",
    "bash",
    "cli",
    "javascript",
    "web-terminal"
  ],
  "author": "ClockWorksProduction Studio",
  "license": "LGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/ClockWorksProduction/CWPs-OpenSource-BASHstyle-Terminal-emulator/issues"
  },
  "homepage": "https://github.com/ClockWorksProduction/CWPs-OpenSource-BASHstyle-Terminal-emulator#readme",
  "documentation": "docs/index.md",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "inquirer": "^8.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "babel-jest": "^30.1.2",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2"
  }
}
